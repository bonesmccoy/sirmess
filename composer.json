{
    "repositories": [
      {
        "type": "vcs",
        "url": "git@github.com:bonesmccoy/messages.git"
      },
      {
        "type": "vcs",
        "url": "git@github.com:bonesmccoy/bongo-fixture-loader.git"
      }
    ],
    "require": {
        "silex/silex": "~1.3",
        "friendsofsymfony/message": "1.0.x-dev",
        "symfony/yaml": "~2.1",
        "symfony/serializer": "~2.3",
        "igorw/config-service-provider": "dev-master",
        "monolog/monolog" : "1.18.0",
        "moriony/silex-mongo-provider": "*",
        "bonesmccoy/messages" : "dev-master"
    },
    "require-dev": {
      "phpspec/phpspec": "~2.0",
      "bonesmccoy/bongo-fixture-loader" : "1.*",
      "phpunit/phpunit" : "4.8.*",
      "symfony/browser-kit": "^2.8",
      "symfony/css-selector": "^2.8"
    },
    "autoload-dev": {
      "psr-4": { "tests\\": "tests/" }
    },
    "config": {
      "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "": "src"
        }
    }
}
